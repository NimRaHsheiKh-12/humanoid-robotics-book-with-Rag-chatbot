{
  "name": "my-humanoid-book",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://NimRaHsheiKh-12.github.io/humanoid-robotics-book-with-Rag-chatbot",
  "scripts": {
    "start": "docusaurus start",
    "start:frontend": "node ./scripts/start-frontend.mjs",
    "start:backend": "python -m uvicorn backend.main:app --host 127.0.0.1 --port 8000",
    "start-dev": "concurrently --kill-others-on-fail \"npm run start:frontend\" \"npm run start:backend\"",
    "start-logs": "concurrently --raw \"npm run start:frontend\" \"npm run start:backend\"",
    "build": "  docusaurus build",
    "deploy": "gh-pages -d build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@chatscope/chat-ui-kit-react": "^2.1.1",
    "@chatscope/chat-ui-kit-styles": "^1.4.0",
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "get-port": "^7.1.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
